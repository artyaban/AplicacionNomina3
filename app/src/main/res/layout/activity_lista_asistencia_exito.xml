<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.artyaban.aplicacionnomina.lista_asistencia_exito">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="LISTA CERRADA !"
        android:id="@+id/textView39"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="CON EXITO!!!"
        android:id="@+id/textView40"
        android:layout_below="@+id/textView39"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:src="@drawable/abc_btn_check_to_on_mtrl_015"
        android:tint="#009a48"
        android:maxHeight="@dimen/abc_panel_menu_list_width"
        android:layout_alignBottom="@+id/textView39"
        android:layout_toRightOf="@+id/textView39"
        android:layout_toEndOf="@+id/textView39" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:src="@drawable/exito"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/imageView4"
        android:layout_toStartOf="@+id/imageView4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONTINUAR"
        android:id="@+id/button15"
        android:layout_marginBottom="40dp"
        android:layout_alignParentBottom="true"
        android:onClick="continuar"
        android:background="#009c48"
        android:layout_toLeftOf="@+id/imageView4"
        android:layout_alignLeft="@+id/textView39"
        android:layout_alignStart="@+id/textView39"
        android:textColor="#ffffff" />
</RelativeLayout>
